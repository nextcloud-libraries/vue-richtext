{
	"name": "@juliushaertl/vue-richtext",
	"version": "1.0.1",
	"description": "Vue component for rich content strings including markdown support.",
	"keywords": [
		"vuejs",
		"components",
		"markdown"
	],
	"homepage": "https://github.com/juliushaertl/vue-richtext",
	"bugs": "https://github.com/juliushaertl/vue-richtext/issues",
	"repository": "https://github.com/juliushaertl/vue-richtext",
	"license": "AGPL-3.0",
	"author": "Julius HÃ¤rtl <jus@bitgrid.net>",
	"main": "dist/vue-richtext.js",
	"files": [
		"LICENSE",
		"README.md",
		"dist"
	],
	"scripts": {
		"build": "NODE_ENV=production vite build",
		"docs:build": "vitepress build docs",
		"docs:dev": "vitepress dev docs",
		"lint": "eslint --ext .js,.vue src",
		"lint:fix": "eslint --ext .js,.vue src --fix",
		"stylelint": "stylelint src",
		"stylelint:fix": "stylelint src --fix",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"watch": "NODE_ENV=development vite"
	},
	"browserslist": [
		"extends @nextcloud/browserslist-config"
	],
	"jest": {
		"moduleFileExtensions": [
			"js",
			"vue"
		],
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1"
		},
		"snapshotSerializers": [
			"<rootDir>/node_modules/jest-serializer-vue"
		],
		"testEnvironment": "jsdom",
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest",
			".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
		}
	},
	"dependencies": {
		"vue": "^2.6.14"
	},
	"devDependencies": {
		"@vue/test-utils": "^1.3.0",
		"clone": "^2.1.2",
		"eslint": "^7.32.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-nextcloud": "^0.3.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.2.0",
		"eslint-plugin-standard": "^5.0.0",
		"eslint-plugin-vue": "^7.20.0",
		"jest": "^27.4.7",
		"jest-serializer-vue": "^2.0.2",
		"prettier-eslint": "^13.0.0",
		"rehype-add-classes": "^1.0.0",
		"rehype-react": "^6.2.1",
		"remark-breaks": "2.0.2",
		"remark-disable-tokenizers": "^1.1.0",
		"remark-external-links": "^8.0.0",
		"remark-parse": "^8.0.3",
		"remark-rehype": "^8.1.0",
		"sass": "^1.52.1",
		"stylelint": "^14.2.0",
		"stylelint-config-recommended-scss": "^5.0.2",
		"stylelint-scss": "^4.1.0",
		"unified": "^9.2.2",
		"vite": "^2.9.9",
		"vite-plugin-vue2": "^2.0.1",
		"vitepress": "^0.22.4",
		"vue-jest": "^3.0.7",
		"vue-server-renderer": "^2.6.14",
		"vue-template-compiler": "^2.6.14"
	},
	"peerDependencies": {
		"vue": "^2.6.14"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"library": "dist/vue-richtext.js"
}
