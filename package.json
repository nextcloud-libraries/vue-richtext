{
	"name": "@juliushaertl/vue-richtext",
	"version": "1.0.1",
	"description": "Vue component for rich content strings including markdown support.",
	"keywords": [
		"vuejs",
		"components",
		"markdown"
	],
	"homepage": "https://github.com/juliushaertl/vue-richtext",
	"bugs": "https://github.com/juliushaertl/vue-richtext/issues",
	"repository": "https://github.com/juliushaertl/vue-richtext",
	"license": "AGPL-3.0",
	"author": "Julius HÃ¤rtl <jus@bitgrid.net>",
	"module": "dist/index.js",
	"main": "dist/index.js",
	"library": "dist/vue-richtext.js",
	"files": [
		"LICENSE",
		"README.md",
		"dist"
	],
	"scripts": {
		"build": "NODE_ENV=production vite build",
		"watch": "NODE_ENV=development vite",
		"docs": "NODE_ENV=production vite --config vite.config.docs.js build",
		"lint": "eslint --ext .js,.vue src",
		"lint:fix": "eslint --ext .js,.vue src --fix",
		"stylelint": "stylelint src",
		"stylelint:fix": "stylelint src --fix",
		"test": "jest",
		"test:coverage": "jest --coverage"
	},
	"browserslist": [
		"extends @nextcloud/browserslist-config"
	],
	"jest": {
		"moduleFileExtensions": [
			"js",
			"vue"
		],
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1"
		},
		"snapshotSerializers": [
			"<rootDir>/node_modules/jest-serializer-vue"
		],
		"testEnvironment": "jsdom",
		"transformIgnorePatterns": [],
		"transform": {
			"^.+\\.js$": "babel-jest",
			"^.+\\.vue$": "@vue/vue2-jest"
		}
	},
	"dependencies": {
		"vue": "^2.7.8"
	},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@babel/eslint-parser": "^7.18.9",
		"@babel/preset-env": "^7.18.10",
		"@nextcloud/browserslist-config": "^2.3.0",
		"@vue/test-utils": "^1",
		"@vue/vue2-jest": "28",
		"@vuepress/plugin-register-components": "^1.9.7",
		"babel-core": "^6.26.3",
		"babel-jest": "^28.1.3",
		"clone": "^2.1.2",
		"core-js": "^3.24.1",
		"eslint": "^8.21.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-n": "^15.2.4",
		"eslint-plugin-nextcloud": "^0.3.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-vue": "^9.3.0",
		"jest": "^28.1.3",
		"jest-environment-jsdom": "^28.1.3",
		"jest-serializer-vue": "^2.0.2",
		"mdast-util-from-markdown": "^1.2.0",
		"mdast-util-gfm-autolink-literal": "^1.0.2",
		"mdast-util-to-string": "^3.1.0",
		"micromark-extension-gfm-autolink-literal": "^1.0.3",
		"prettier-eslint": "^15.0.1",
		"rehype-add-classes": "^1.0.0",
		"rehype-react": "^7.1.1",
		"remark-breaks": "3.0.2",
		"remark-disable-tokenizers": "^1.1.0",
		"remark-external-links": "^9.0.1",
		"remark-parse": "^10.0.1",
		"remark-rehype": "^10.1.0",
		"sass": "^1.54.4",
		"stylelint": "^14.9.1",
		"stylelint-config-recommended-scss": "^6.0.0",
		"stylelint-scss": "^4.3.0",
		"unified": "^10.1.2",
		"unist-builder": "^3.0.0",
		"unist-util-visit": "^4.1.0",
		"vite": "^2.9.15",
		"vite-plugin-markdown": "^2.1.0",
		"vite-plugin-vue2": "^2.0.2",
		"vitepress": "^0.22.4",
		"vue-jest": "^3.0.7",
		"vue-server-renderer": "^2.7.8",
		"vue-template-compiler": "^2.7.8",
		"vuepress": "^1.9.7"
	},
	"peerDependencies": {
		"vue": "^2.7.8"
	},
	"engines": {
		"node": ">=10.0.0"
